{
    "family": "blazor_app_task",
    "containerDefinitions": [
        {
            "name": "blazor_app_container",
            "image": "448049819404.dkr.ecr.us-east-1.amazonaws.com/thecodeiackiller/exercise-api-app:5f9eaa49ff7c49b4750654c6c2f925a98c7c9f7c",
            "cpu": 256,
            "memory": 307,
            "memoryReservation": 256,
            "portMappings": [
                {
                    "name": "blazor_app_container-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "API_BASE_URL",
                    "value": "http://localhost:5000"
                },
                {
                    "name": "DB_CONNECTION_STRING",
                    "value": "Server=localhost;Database=fitsked;User Id=sa;Password=$DB_PASSWORD;Encrypt=False;TrustServerCertificate=true;MultipleActiveResultSets=true"
                },
                {
                    "name": "AWS_EC2",
                    "value": "true"
                },
                {
                    "name": "ASPNETCORE_ENVIRONMENT",
                    "value": "Development"
                },
                {
                    "name": "EF_MIGRATE",
                    "value": "true"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:448049819404:secret:sql_server_db_pass-PYQrFI"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/blazor_app_task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        },
        {
            "name": "sqlserver",
            "image": "mcr.microsoft.com/mssql/server:2022-latest",
            "cpu": 512,
            "memory": 1024,
            "memoryReservation": 1020,
            "portMappings": [
                {
                    "name": "sqlserver-1433-tcp",
                    "containerPort": 1433,
                    "hostPort": 1433,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "MSSQL_PID",
                    "value": "Developer"
                },
                {
                    "name": "ACCEPT_EULA",
                    "value": "Y"
                },
                {
                    "name": "MSSQL_TCP_PORT",
                    "value": "1433"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "MSSQL_SA_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:448049819404:secret:sql_server_db_pass-PYQrFI"
                }
            ],
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "/opt/mssql-tools18/bin/sqlcmd -C -S localhost -U sa -P \"$MSSQL_SA_PASSWORD\" -Q \"SELECT 1\" || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 45
            },
            "systemControls": []
        },
        {
            "name": "exercise-api",
            "image": "448049819404.dkr.ecr.us-east-1.amazonaws.com/thecodeiackiller/exercise-api-api:latest",
            "cpu": 154,
            "memory": 205,
            "memoryReservation": 154,
            "portMappings": [
                {
                    "name": "exercise-api-8080-tcp",
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "AWS_EC2",
                    "value": "true"
                },
                {
                    "name": "ASPNETCORE_ENVIRONMENT",
                    "value": "Development"
                },
                {
                    "name": "EF_MIGRATE",
                    "value": "true"
                },
                {
                    "name": "API_DB_CONNECTION_STRING",
                    "value": "Server=localhost;Database=exercises-api;User Id=sa;Password=$DB_PASSWORD;Encrypt=False;TrustServerCertificate=True;MultipleActiveResultSets=true"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:448049819404:secret:sql_server_db_pass-PYQrFI"
                }
            ],
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::448049819404:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::448049819404:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "922",
    "memory": "1536",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
